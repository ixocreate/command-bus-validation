{
    "name": "ixocreate/command-bus-validation",
    "description": "Validation for the IXOCREATE Command-Bus",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/contract": "dev-develop",
        "ixocreate/command-bus": "dev-develop",
        "ixocreate/validation": "dev-develop"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.5",
        "phpunit/phpunit": "^6.0.6",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\CommandBusValidation\\": "src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
